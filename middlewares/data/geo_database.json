  "blocked_regions": {
    "description": "Regiones completamente bloqueadas por seguridad",
    "high_risk": [
      "CN", "RU", "KP", "IR", "SY", "AF"
    ],
    "temporary_blocks": []
  },
  
  "access_rules": {
    "description": "Reglas de acceso y políticas de seguridad",
    "max_attempts_per_ip": 5,
    "lockout_duration_minutes": 30,
    "whitelist_bypass_attempts": true,
    "log_all_access": true,
    "notify_blocked_access": true,
    "geolocation_required": true,
    "vpn_detection": {
      "enabled": true,
      "action": "log_and_allow",
      "strict_mode": false
    }
  },
  
  "security_headers": {
    "description": "Headers de seguridad para implementar",
    "X-Geo-Country": "required",
    "X-Real-IP": "required", 
    "X-Forwarded-For": "validate",
    "CF-IPCountry": "optional"
  },
  
  "monitoring": {
    "description": "Configuración de monitoreo geográfico",
    "log_path": "logs/geo_access.log",
    "alert_threshold": 100,
    "daily_reports": true,
    "suspicious_patterns": {
      "multiple_countries_same_session": true,
      "rapid_ip_changes": true,
      "blocked_country_attempts": true
    }
  }
}
